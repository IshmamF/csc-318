{ "version": 2, 
"functions": {"api/*.js": {
    "memory": 3009,
    "maxDuration": 30
  }},
"builds": [
    {
        "src": "./index.js",
        "use": "@vercel/node"
    }
],
"routes": [
    {
        "src": "/(.*)",
        "dest": "/"
    }
],
"rewrites": [
    {
     "source": "/(.*)", "destination": "/api" 
    }
    ] 
}